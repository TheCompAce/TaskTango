{
    "groupName": "ProgramCreationGroup",
    "systemPrompt": "groups/system_prompts/ProgramCreationGroup.txt",
    "agents": [
        "CodeGenAgent",
        "TestingAgent",
        "DocumentationAgent",
        "VersionControlAgent",
        "ReviewAgent"
    ],
    "completionIndicators": ["code_completed", "test_passed", "documentation_completed"],
    "variables": {
        "code_completed": false,
        "test_passed": false,
        "documentation_completed": false,
        "code_complexity": "medium",
        "review_standard": "high",
        "documentation_standard": "comprehensive"
    },
    "dynamicAgentAllocation": true,
    "allowAgentCloning": true
}
